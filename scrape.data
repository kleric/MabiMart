Item::create(array('url' => http://wiki.mabinogiworld.com/view/black_dragon%27s_fragment,'name' => 'Black Dragon's Fragment','imgurl' => 'http://wiki.mabinogiworld.com/images/b/b9/Black_Dragon%27s_Fragment.png','description' => 'Fragments of the Black Dragon that have been dispersed across the world. It is said bringing the fragments back to the Black Dragon's Human Avatar will encourage him to grant you a wish!'));
Item::create(array('url' => http://wiki.mabinogiworld.com/view/black_dragon%27s_wish_gift_box,'name' => 'Black Dragon's Wish Gift Box','imgurl' => 'http://wiki.mabinogiworld.com/images/d/df/Lucky_Box_-_Indigo.png','description' => 'Year of the Dragon gift box obtained by making a wish to the Black Dragon.'));
Item::create(array('url' => http://wiki.mabinogiworld.com/view/black_flame_mare_whistle_(type_1),'name' => 'Black Flame Mare Whistle (Type 1)','imgurl' => 'http://wiki.mabinogiworld.com/images/c/cb/Red_Pet_Whistle.png','description' => 'A unique opportunity to meet a remarkable animal, the majestic Black Flame Mare!'));
Item::create(array('url' => http://wiki.mabinogiworld.com/view/black_flame_mare_whistle_(type_2),'name' => 'Black Flame Mare Whistle (Type 2)','imgurl' => 'http://wiki.mabinogiworld.com/images/7/7f/Purple_Pet_Whistle.png','description' => 'A whistle that calls the regal Black Flamemare!'));
