Item::create(array('url' => http://wiki.mabinogiworld.com/view/baby_pink_package_set,'name' => 'Baby Pink Package Set','imgurl' => 'http://wiki.mabinogiworld.com/images/5/56/Gift_Box.png','description' => 'This package contains baby pink items.'));
